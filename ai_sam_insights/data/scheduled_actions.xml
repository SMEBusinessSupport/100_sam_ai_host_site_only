<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Weekly Ecosystem Scan -->
    <record id="ir_cron_weekly_scan" model="ir.cron">
        <field name="name">SAM Insights: Weekly Ecosystem Scan</field>
        <field name="model_id" ref="model_insights_scan"/>
        <field name="state">code</field>
        <field name="code">model.action_run_quick_scan()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">weeks</field>
        <field name="active" eval="False"/>
    </record>

</odoo>
